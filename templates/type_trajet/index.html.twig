{% extends 'base.html.twig' %}

{% block title %}TypeTrajet index{% endblock %}

{% block body %}
    <h1>TypeTrajet index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Typet</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for type_trajet in type_trajets %}
            <tr>
                <td>{{ type_trajet.id }}</td>
                <td>{{ type_trajet.typet }}</td>
                <td>
                    <a href="{{ path('app_type_trajet_show', {'id': type_trajet.id}) }}">show</a>
                    <a href="{{ path('app_type_trajet_edit', {'id': type_trajet.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_type_trajet_new') }}">Create new</a>
{% endblock %}
